{
    "url": "http://localhost:2368",
    "storage": {
      "active": "gcloud",
      "gcloud": {
        "projectID": "your-project-ud",
        "keyFilename": "gcloud-storage-account.json",
        "bucket": "your-bucket-name"
      }
    },
    "database": {
      // This should match production, for initial deployment purposes.
    },
    "auth": {
        "type": "password"
    },
    "paths": {
        "contentPath": "this should be the absolute path to the directory in which this config file resides."
    },
    "privacy": {
        "useRpcPing": false,
        "useUpdateCheck": true
    },
    "useMinFiles": false,
    "caching": {
        "theme": {
            "maxAge": 0
        },
        "admin": {
            "maxAge": 0
        }
    }
}
